    "keywords": [
        "laravel",
        "menus",
        "generator",
        "url",
        "routes",
        "views"
    ],
    "homepage": "https://3x1.io",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "io3x1\\FilamentMenus\\": "src/"
        }
    },
    "authors": [
        {
            "name": "3x1",
            "email": "info@3x1.io",
            "role": "CEO"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.9",
        "spatie/laravel-package-tools": "^1.10",
        "spatie/laravel-sitemap": "^6.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
